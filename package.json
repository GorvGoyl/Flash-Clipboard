    "build:mac": "build --mac dir",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "files": [
    "./icon.ico"
  ],
  "repository" :
  { "type" : "git"
  , "url" : "https://github.com/JerryGoyal/MultiCopy-Paste.git"
  },
  "publish": [{
    "provider": "github",
    "owner": "jerrygoyal",
    "repo": "MultiCopy-Paste"
}],
  "build": {
    "productName": "Multicopy Paste",
    "copyright": "Copyright Â© 2017 Gourav Goyal",
    "appId": "com.jerryapps.multicopypaste",
    
