{
  "name": "multicopy-paste",
  "productName": "Multicopy Paste",
  "author": "JerryGoyal",
  "license": "ISC",
  "version": "0.2.0",
  "description": "Software to maintain list of recently copied text which can be pasted later using a shortcut",
  "homepage": "https://github.com/JerryGoyal/MultiCopy-Paste",
  "main": "main.js",
  "keywords": [
    "desktop",
    "electron",
    "electron-app",
    "clipboard",
    "multi copy paste",
    "multicopy paste",
    "mac",
    "windows",
    "linux",
    "software"
  ],
  "scripts": {
    "start": "./node_modules/.bin/electron ./main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "publish": "node_modules/.bin/build -p always",
    "package": "npx electron-builder"
  },
  "files": [
    "./icon.ico"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JerryGoyal/MultiCopy-Paste.git"
  },
  "build": {
    "productName": "Multicopy Paste",
    "copyright": "Copyright Â© 2017 Gourav Goyal",
    "appId": "com.jerrymadeapps.multicopypaste",
    "win": {
      "icon": "icon.ico",
      "publisherName": "jerrymadeapps"
    },
    "nsis": {
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "deleteAppDataOnUninstall": true
    }
  },
  "dependencies": {
    "electron-json-storage": "^4.0.0",
    "electron-updater": "^2.15.1",
    "robotjs": "^0.4.7"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "19.43.3"
  }
}
